/* pages/index/index.wxss */
/*整个网页*/
.all{width: 100%; height: auto;background-color: white;}
page { font-family: 'Microsoft YaHei'; }

.relative{position: relative}
.Center{margin: 0px auto;}
.border{border-bottom: 1px solid gainsboro;border-top: 1px solid gainsboro;}

/*头部*/
.header{width: 100%; height: 400rpx;background-color: red;}
.header image{width: 100%;height: 100%;}

/*头部第一块*/
.head1{width: 90%;position: absolute;background-color: white;left: 5%;top: 65%;border-radius: 12rpx;box-shadow: gray 0rpx 5rpx 10rpx;}
/*上*/
.head1 .top{width: 80%;margin-top: 10rpx;margin-bottom: 8rpx;}
.head1 .top .left{width: 70%;display: inline-block;padding: 20rpx; box-sizing: border-box;font-size: 33rpx;}
.head1 .top .right{width: 30%;display: none;padding: 20rpx;border-right: 1px gainsboro solid; box-sizing: border-box;background-color: red;}
/*中*/
.head1 .middle{width: 80%;text-align: center;display:table;}
.head1 .middle .section{align-items: center;justify-content: center;display: table-cell;vertical-align: middle;}
.head1 .middle .section .picker .date{font-size:24rpx;color:gray;}
.head1 .middle .section .picker .dateTime{font-size:26rpx;}
.head1 .middle .night{width: 20%;display: inline-block;line-height: 161rpx;position: relative;text-align: center;}
.head1 .middle .night .nightNum{font-size:28rpx;}
.head1 .middle .night .em3{display: block;height: 40rpx;background-color: gainsboro;width: 1px;position: absolute;top: 10%;left: 50%;}
.head1 .middle .night .em4{display: block;height: 40rpx;background-color: gainsboro;width: 1px;position: absolute;top: 68%;left: 50%;}
/*下*/
.head1 .bottom{width: 80%;}
.head1 .bottom button{width: 100%;color: white ;line-height: 80rpx;margin-top: 25rpx;margin-bottom: 52rpx;
  box-shadow: 0rpx 6rpx 3rpx #A8DBFB;font-size: 28rpx; letter-spacing: 4rpx;}
/*主体*/
.main{width: 100%;height: 400rpx;margin-top: 46%;}
.main1{width: 80%; height: 80rpx;margin: 0px auto;line-height: 80rpx;text-align: center;}
.main1 .em1{display: block;width: 30%; height: 1px;background-color: gainsboro;margin-top: 7%;float: left;}
.main1 .em2{display: block;width: 30%; height: 1px;background-color: gainsboro;margin-top: 7%;float: right;}
.main1 .brand{ font-size: 28rpx;}
.main2{width: 120rpx;height: 165rpx;margin: 0px auto;
margin-top: 3%;float: left;margin-left: 13%;}
.main2 .one{
  width: 120rpx;width: 120rpx;}
.main2 .one image{width: 120rpx;height: 120rpx;border-radius: 50%;}
.main2 .two{width: 136rpx;font-size: 28rpx;width: 100%;height: 40rpx;line-height: 40rpx;text-align: center;margin-top: 5rpx;}
